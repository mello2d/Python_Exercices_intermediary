print("Início")
for numero in range(5):
    print(numero)
print("Fim")

print("Início do segundo range")
for numero2 in range(1,5):
    print(numero2)
print("Fim do segundo range")

print("Início do terceiro range")
for numero3 in range(40, 101 , 5):
    print(numero3)
print("Fim do terceiro range")

print("Início do quarto range")
for numero4 in range(50, 40 , -2):
    print(numero4)
print("Fim do quarto range")